{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "library": "jquery-timeago@1.6.7",
      "destination": "wwwroot/lib/jquery-timeago/",
      "files": [
        "jquery.timeago.min.js"
      ]
    },
    {
      "library": "limonte-sweetalert2@11.4.17",
      "destination": "wwwroot/lib/limonte-sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "fullcalendar@3.4.0",
      "destination": "wwwroot/lib/fullcalendar/",
      "files": [
        "fullcalendar.min.css",
        "fullcalendar.print.min.css",
        "fullcalendar.min.js"
      ]
    }
  ]
}